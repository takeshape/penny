{
  "name": "@takeshape/penny",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "shape promote-branch --lookup-pr --production-only --nofail",
    "build": "npm run graphql:typegen && next build",
    "postbuild": "next-sitemap",
    "build-storybook": "storybook build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev | pino-pretty --colorize",
    "export": "next build && next export",
    "graphql:typegen": "export NEXT_PUBLIC_BRANCH_TAKESHAPE_API_URL=\"$(shape get-branch-url)\" && npm run graphql:typegen:takeshape && npm run graphql:typegen:storefront",
    "graphql:typegen:storefront": "graphql-codegen --config ./codegen.storefront.config.js",
    "graphql:typegen:takeshape": "graphql-codegen --config ./codegen.takeshape.config.js",
    "graphql:validate": "export NEXT_PUBLIC_BRANCH_TAKESHAPE_API_URL=\"$(shape get-branch-url)\" && npm run graphql:validate:takeshape && npm run graphql:validate:storefront",
    "graphql:validate:storefront": "graphql-validate --config ./codegen.storefront.config.js",
    "graphql:validate:takeshape": "graphql-validate --config ./codegen.takeshape.config.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "prepare-env": "shape prepare-env",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "takeshape:create-branch": "shape create-branch",
    "takeshape:delete-branch": "shape delete-branch",
    "takeshape:get-branch-url": "shape get-branch-url",
    "takeshape:post-checkout-hook": "shape post-checkout-hook",
    "takeshape:post-merge-hook": "shape post-merge-hook",
    "takeshape:promote-branch": "shape promote-branch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@apollo/client": "^3.7.13",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@next/bundle-analyzer": "^13.4.4",
    "@next/env": "^13.4.4",
    "@sentry/node": "^7.50.0",
    "@sentry/react": "^7.50.0",
    "@takeshape/next-auth-all-access": "^0.4.3",
    "@takeshape/routing": "^9.14.1",
    "abortcontroller-polyfill": "^1.7.5",
    "card-validator": "^8.1.1",
    "date-fns": "^2.30.0",
    "graphql": "^16.6.0",
    "imask": "^6.6.1",
    "jotai": "^2.2.0",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "next-plugin-preval": "^1.2.5",
    "next-pwa": "5.6.0",
    "next-recaptcha-v3": "^1.1.5",
    "next-seo": "^6.0.0",
    "node-fetch": "^3.3.1",
    "nookies": "^2.5.2",
    "pino": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-imask": "^6.6.1",
    "react-phone-number-input": "^3.2.20",
    "react-star-ratings": "^2.3.0",
    "sharp": "^0.32.1",
    "slug": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/nextjs": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/testing-library": "0.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@takeshape/graphql-validate": "^1.0.1",
    "@takeshape/shape-tools": "^0.7.6",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.0",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-star-ratings": "^2.3.0",
    "@types/slug": "^5.0.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "cypress": "^12.11.0",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "graphql-request": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.2",
    "msw-storybook-addon": "^1.8.0",
    "next-sitemap": "^4.1.3",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "storybook": "^7.0.20",
    "tailwindcss": "^3.3.2",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^3.9.0",
    "typescript": "^4.9.5",
    "webpack": "^5.81.0"
  },
  "packageManager": "npm@9.5.1",
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "msw": {
    "workerDirectory": "mocks"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "$next"
  }
}
