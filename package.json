    "nookies": "^2.5.2",
    "pino": "^8.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.4.0",
    "react-phone-number-input": "^3.2.6",
    "react-star-ratings": "^2.3.0",
    "sharp": "^0.30.7",
    "slug": "^5.3.0"
