{
  "name": "@takeshape/penny",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "./scripts/prebuild.sh",
    "build": "export NEXT_PUBLIC_BRANCH_TAKESHAPE_API_URL=\"$(cat .current-branch-url)\" && npm run graphql:typegen && next build",
    "postbuild": "next-sitemap",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev",
    "export": "next build && next export",
    "graphql:typegen": "npm run graphql:typegen:takeshape && npm run graphql:typegen:storefront",
    "graphql:typegen:storefront": "graphql-codegen --config ./codegen.storefront.config.js",
    "graphql:typegen:takeshape": "graphql-codegen --config ./codegen.takeshape.config.js",
    "graphql:validate": "npm run graphql:validate:takeshape && npm run graphql:validate:storefront",
    "graphql:validate:storefront": "graphql-validate --config ./codegen.storefront.config.js",
    "graphql:validate:takeshape": "graphql-validate --config ./codegen.takeshape.config.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare-env": "./scripts/prepare-env.mjs",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@next/bundle-analyzer": "^13.0.7",
    "@next/env": "^13.0.7",
    "@sentry/node": "^7.27.0",
    "@sentry/react": "^7.27.0",
    "@takeshape/next-auth-all-access": "^0.3.3",
    "@takeshape/routing": "^8.254.0",
    "abortcontroller-polyfill": "^1.7.5",
    "card-validator": "^8.1.1",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "imask": "^6.4.3",
    "jotai": "^1.11.2",
    "next": "^13.0.7",
    "next-auth": "^4.18.6",
    "next-plugin-preval": "^1.2.4",
    "next-pwa": "5.6.0",
    "next-recaptcha-v3": "^1.1.1",
    "next-seo": "^5.15.0",
    "node-fetch": "^3.3.0",
    "nookies": "^2.5.2",
    "pino": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-icons": "^4.7.1",
    "react-imask": "^6.4.3",
    "react-phone-number-input": "^3.2.13",
    "react-star-ratings": "^2.3.0",
    "sharp": "^0.31.2",
    "slug": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "0.0.13",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@takeshape/graphql-validate": "^1.0.0",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/react-star-ratings": "^2.3.0",
    "@types/slug": "^5.0.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "cypress": "^10.11.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.8",
    "inquirer": "^9.1.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "msw": "^0.49.2",
    "msw-storybook-addon": "^1.6.3",
    "next-sitemap": "^3.1.42",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "storybook-addon-jotai": "^0.0.6",
    "storybook-addon-next-router": "^4.0.2",
    "tailwindcss": "^3.2.4",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^3.4.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "packageManager": "npm@8.19.2",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "msw": {
    "workerDirectory": "mocks"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "$next"
  }
}
